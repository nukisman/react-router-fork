{
  "name": "react-router-fork",
  "version": "4.2.0",
  "description": "Simpler routing based on react-router",
  "keywords": [
    "react",
    "router",
    "fork",
    "state",
    "typescript"
  ],
  "homepage": "https://github.com/nukisman/react-router-fork",
  "readme": "./README.md",
  "author": "nukisman (Alexander Nuikin)",
  "license": "MIT",
  "repository": "nukisman/react-router-fork",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "lint": "yarn lint-file .",
    "lint-file": "eslint --ext=js,jsx,ts,tsx --ignore-pattern '!.eslintrc.js'",
    "prestart": "yarn run lint --fix",
    "pretest": "yarn run lint --fix",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hooks-pure": "^2.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@svgr/rollup": "^4.3.2",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.142",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-is": "^16.7.1",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^6.0.0",
    "eslint": "^6.5.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "prettier": "^1.18.2",
    "react-scripts": "^3.2.0",
    "rollup": "^1.23.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.24.2",
    "rollup-plugin-url": "^2.2.2",
    "typescript": "^3.6.3"
  }
}
